#include<iostream>

using namespace std;

int main()  {
    int weekday;
    cin>>weekday;
    // switch statement used to define multiple cases
     switch (weekday)
     {
         case 1:
         cout << "Monday";
         break;    // break is used to ensure all cases don't get executed.
         case 2:
         cout << "Tuesday";
         break;
         case 3:
         cout << "Wednesday";
         break;
         case 4:
         cout << "Thursday";
         break;
         case 5:
         cout << "Friday";
         break;
         case 6:
         cout << "Saturday";
         break;
         case 7:
         cout << "Sunday";
         break;
         
     }
    return 0;
}
